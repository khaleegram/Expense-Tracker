
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // Allow public read/write access to expenses for prototyping
    match /expenses/{expenseId} {
      allow read, write: if true;
    }

    // Allow public read/write access to items for prototyping
    match /items/{itemId} {
      allow read, write: if true;
    }
  }
}
